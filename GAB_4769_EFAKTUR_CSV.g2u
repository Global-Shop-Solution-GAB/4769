	F.Data.DataTable.AddColumn("dtCSV","Delete","Long")
	F.Data.DataTable.AddRow("dtCSV","Faktur","")

	F.Intrinsic.String.Build("select customer from gab_4769_efaktur_hf where id = {0}",V.Local.iID,V.Local.sSQL)
