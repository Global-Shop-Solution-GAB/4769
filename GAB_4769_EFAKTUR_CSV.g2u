	F.Data.DataTable.AddColumn("dtCSV","Faktur","String")
	F.Data.DataTable.AddColumn("dtCSV","FakturDate","Date")
	F.Data.DataTable.AddColumn("dtCSV","ID","Long")
	F.Data.DataTable.AddColumn("dtCSV","Delete","Long")
